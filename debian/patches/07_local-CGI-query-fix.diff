diff -urNad elinks-0.11.3~/src/protocol/uri.c elinks-0.11.3/src/protocol/uri.c
--- elinks-0.11.3~/src/protocol/uri.c	2007-04-16 02:21:03.000000000 +0530
+++ elinks-0.11.3/src/protocol/uri.c	2008-01-06 23:51:53.000000000 +0530
@@ -149,7 +149,7 @@
