INTLOBJS = @INTLOBJS@
INTL_LIBTOOL_SUFFIX_PREFIX = 
JW = 
LIBDIR = /usr/local/lib
LOCALEDIR = /usr/local/share/locale
LUA_CFLAGS = 
LUA_LIBS = 
MKINSTALLDIRS = $(PATHPREFIX)config/mkinstalldirs
MSGFMT = /usr/bin/msgfmt
OPENSSL_CFLAGS = 
PACKAGE = elinks
PERL_CFLAGS = 
PERL_LIBS = 
POD2HTML = /usr/bin/pod2html
PYTHON_CFLAGS = 
PYTHON_LIBS = 
RANLIB = ranlib
RUBY_CFLAGS = 
RUBY_LIBS = 
SEE_CFLAGS = 
SPARSE = 
SPIDERMONKEY_CFLAGS = 
SPIDERMONKEY_LIBS = 
VERSION = 0.12.GIT
